{
	"build": {
		"dockerfile": "ubuntu.dockerfile",
		"args": {
			"USERNAME": "pymake",
			"USER_UID": "1000",
			"USER_GID": "1000"
		}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"eamodio.gitlens",
				"EditorConfig.EditorConfig",
				"ms-python.python",
				"ms-vscode.cmake-tools",
				"ryanluker.vscode-coverage-gutters",
				"streetsidesoftware.code-spell-checker",
				"yzhang.markdown-all-in-one"
			]
		}
	},
	"containerEnv": {
		"PYTHONPATH": "/workspaces/PyMake/source"
	},
	"forwardPorts": [
		8000
	]
}
